<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<numbers>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0;-#,##0</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##0 %</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
	</numbers>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>yyyy-MM-dd tt</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>h:mm:ss</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>