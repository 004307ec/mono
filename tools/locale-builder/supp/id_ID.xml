	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<default type="medium"/>
					<dateFormatLength type="medium">
						<dateFormat >
							<pattern>dd MMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>dd/MM/yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>